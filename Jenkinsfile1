pipeline{
agent any
stages{
stage('one'){
when {
expression{ env.BRANCH_NAME.equals('ut') }
}
  steps{
echo "This is ut branch"
}

}
stage('one1'){
when {
environment name:'BRANCH_NAME', value: 'ut'
}
  steps{
script{
if(env.BRANCH_NAME == "ut"){
echo "my branch name is ut"
}else{ 
echo "my branch name not is ut"
}
}
}

}

}


}
